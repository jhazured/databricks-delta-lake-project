# Development Environment Configuration - Delta Lake Project
# Copy this file to dev.env and update with your values

# =============================================================================
# DATABRICKS CONFIGURATION
# =============================================================================
DATABRICKS_HOST=https://your-dev-workspace.cloud.databricks.com
DATABRICKS_TOKEN=your-dev-personal-access-token
DATABRICKS_WORKSPACE_ID=your-dev-workspace-id
DATABRICKS_CLUSTER_ID=your-dev-cluster-id

# =============================================================================
# CLOUD STORAGE CONFIGURATION
# =============================================================================
# AWS S3
AWS_ACCESS_KEY_ID=your-dev-aws-access-key
AWS_SECRET_ACCESS_KEY=your-dev-aws-secret-key
S3_BUCKET_NAME=your-dev-delta-lake-bucket
S3_REGION=us-west-2
S3_ENDPOINT_URL=

# Azure Data Lake Storage
AZURE_STORAGE_ACCOUNT=yourdevstorageaccount
AZURE_STORAGE_KEY=your-dev-storage-key
ADLS_CONTAINER=dev-delta-lake-data
AZURE_TENANT_ID=your-tenant-id
AZURE_CLIENT_ID=your-client-id
AZURE_CLIENT_SECRET=your-client-secret

# Google Cloud Storage
GOOGLE_APPLICATION_CREDENTIALS=path/to/dev-service-account.json
GCS_BUCKET_NAME=your-dev-delta-lake-bucket
GCS_PROJECT_ID=your-dev-project-id

# =============================================================================
# DATABASE CONFIGURATION
# =============================================================================
DB_HOST=your-dev-database-host
DB_PORT=5432
DB_NAME=delta_lake_dev
DB_USER=your-dev-db-user
DB_PASSWORD=your-dev-db-password
DB_SSL_MODE=require

# =============================================================================
# MLFLOW CONFIGURATION
# =============================================================================
MLFLOW_TRACKING_URI=databricks
MLFLOW_EXPERIMENT_NAME=/delta-lake-ml-experiments/dev
MLFLOW_MODEL_REGISTRY_URI=databricks

# =============================================================================
# FEATURE STORE CONFIGURATION
# =============================================================================
FEATURE_STORE_NAME=delta_lake_feature_store_dev
FEATURE_STORE_CATALOG=delta_lake_catalog_dev
FEATURE_STORE_SCHEMA=features

# =============================================================================
# MONITORING CONFIGURATION
# =============================================================================
PROMETHEUS_ENDPOINT=http://localhost:9090
GRAFANA_ENDPOINT=http://localhost:3000
GRAFANA_API_KEY=your-grafana-api-key
ALERTMANAGER_ENDPOINT=http://localhost:9093

# =============================================================================
# LOGGING CONFIGURATION
# =============================================================================
LOG_LEVEL=DEBUG
LOG_FORMAT=json
LOG_FILE_PATH=/var/log/logistics-platform
LOG_ROTATION_SIZE=100MB
LOG_RETENTION_DAYS=30

# =============================================================================
# SECURITY CONFIGURATION
# =============================================================================
ENCRYPTION_KEY=your-dev-encryption-key
JWT_SECRET_KEY=your-dev-jwt-secret
SESSION_TIMEOUT=3600
MAX_LOGIN_ATTEMPTS=5
LOCKOUT_DURATION=900

# =============================================================================
# PERFORMANCE CONFIGURATION
# =============================================================================
MAX_CONCURRENT_JOBS=10
JOB_TIMEOUT_SECONDS=3600
MEMORY_LIMIT_GB=8
CPU_LIMIT_CORES=4

# =============================================================================
# FEATURE FLAGS
# =============================================================================
ENABLE_REAL_TIME_PROCESSING=true
ENABLE_ML_MODEL_SERVING=true
ENABLE_DATA_QUALITY_MONITORING=true
ENABLE_COST_OPTIMIZATION=true
ENABLE_AUTO_SCALING=true

# =============================================================================
# EXTERNAL INTEGRATIONS
# =============================================================================
# Kafka
KAFKA_BOOTSTRAP_SERVERS=localhost:9092
KAFKA_SECURITY_PROTOCOL=PLAINTEXT
KAFKA_TOPIC_PREFIX=delta-lake-dev

# Event Hubs
EVENT_HUBS_CONNECTION_STRING=your-dev-eventhubs-connection-string
EVENT_HUBS_CONSUMER_GROUP=delta-lake-consumer-group

# Slack
SLACK_WEBHOOK_URL=your-dev-slack-webhook-url
SLACK_CHANNEL=#delta-lake-dev

# Email
SMTP_HOST=smtp.your-org.com
SMTP_PORT=587
SMTP_USERNAME=your-dev-smtp-user
SMTP_PASSWORD=your-dev-smtp-password
SMTP_FROM_EMAIL=delta-lake-dev@your-org.com

# =============================================================================
# ENVIRONMENT SPECIFIC SETTINGS
# =============================================================================
ENVIRONMENT=dev
DEBUG=true
ENABLE_DEBUG_LOGGING=true
ENABLE_PROFILING=false
ENABLE_METRICS_COLLECTION=true
ENABLE_TRACING=true

# =============================================================================
# DATA RETENTION
# =============================================================================
BRONZE_RETENTION_DAYS=90
SILVER_RETENTION_DAYS=365
GOLD_RETENTION_DAYS=730
ARCHIVE_RETENTION_DAYS=2555  # 7 years

# =============================================================================
# COST OPTIMIZATION
# =============================================================================
AUTO_TERMINATION_MINUTES=30
MAX_CLUSTER_SIZE=4
MIN_CLUSTER_SIZE=1
SCALE_UP_THRESHOLD=0.8
SCALE_DOWN_THRESHOLD=0.2
